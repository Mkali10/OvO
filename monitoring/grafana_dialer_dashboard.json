          }
        ]
      },
      {
        "type": "graph",
        "title": "Agents Available",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "dialer_agents_available",
            "refId": "A"
          }
        ]
      },
      {
        "type": "graph",
